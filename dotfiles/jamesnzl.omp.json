{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 2,
	"console_title_template": "{{ .HostName }} {{ .Folder }}",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "session",
					"style": "diamond",
					"foreground": "#caffbe",
					"leading_diamond": "<#bfa8bf>~</> ",
					"template": "{{ .HostName }} <#ffffff>on</>",
					"properties": {
						"display_host": true
					}
				},
				{
					"type": "time",
					"style": "diamond",
					"foreground": "#fdd0ff",
					"template": " {{ .CurrentDate | date .Format }} ",
					"properties": {
						"time_format": "Monday <#ffffff>at</> 3:04 PM"
					}
				},
				{
					"type": "git",
					"style": "diamond",
					"foreground": "#fdd0ff",
					"template": "<#ffffff>and</> {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }} ",
					"properties": {
						"branch_icon": "\ue725 ",
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"fetch_worktree_count": true
					}
				}
			],
			"newline": true
		},
		{
			"type": "prompt",
			"alignment": "right",
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "#caffbe"
				},
				{
					"type": "executiontime",
					"style": "diamond",
					"foreground": "#caffbe",
					"template": "<#ffffff>\udb81\udcc5</> {{ .FormattedMs }}s ",
					"properties": {
						"style": "dallas",
						"threshold": 0
					}
				},
				{
					"type": "sysinfo",
					"style": "diamond",
					"foreground": "#caffbe",
					"template": " <#ffffff>\uf824\u2800</> <#ffffff>CPU:</> {{ round .PhysicalPercentUsed .Precision }}% "
				},
				{
					"type": "sysinfo",
					"style": "diamond",
					"foreground": "#caffbe",
					"template": " <#ffffff>\udb82\uddd1</> <#ffffff>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB "
				}
			],
			"overflow": "break"
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "path",
					"style": "diamond",
					"foreground": "#cff9ff",
					"leading_diamond": "\b<#bfa8bf>~</>",
					"template": " \ue5ff  {{ .Path }}",
					"properties": {
						"folder_icon": "/",
						"folder_separator_icon": " / ",
						"home_icon": "home",
						"style": "agnoster_full"
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "exit",
					"style": "plain",
					"foreground": "#8eff7f",
					"foreground_templates": [
						"{{ if gt .Code 0 }}#ef5350{{ end }}"
					],
					"template": "~ ",
					"properties": {
						"always_enabled": true
					}
				}
			],
			"newline": true
		}
	],
	"osc99": true
}