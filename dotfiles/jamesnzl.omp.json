{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#caffbe",
					"leading_diamond": "<#bfa8bf>~</> ",
					"properties": {
						"display_host": true
					},
					"style": "diamond",
					"template": "{{ .HostName }} <#ffffff>on</>",
					"type": "session"
				},
				{
					"foreground": "#fdd0ff",
					"properties": {
						"time_format": "Monday <#ffffff>at</> 3:04 PM"
					},
					"style": "diamond",
					"template": " {{ .CurrentDate | date .Format }} ",
					"type": "time"
				},
				{
					"foreground": "#fdd0ff",
					"properties": {
						"branch_icon": "\ue725 ",
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"fetch_worktree_count": true
					},
					"style": "diamond",
					"template": "<#ffffff>and</> {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"foreground": "#caffbe",
					"style": "plain",
					"type": "text"
				},
				{
					"foreground": "#caffbe",
					"properties": {
						"style": "dallas",
						"threshold": 0
					},
					"style": "diamond",
					"template": "<#ffffff>\ue601</>  {{ .FormattedMs }}s ",
					"type": "executiontime"
				},
				{
					"foreground": "#caffbe",
					"style": "diamond",
					"template": " <#ffffff>\uf109</>  <#ffffff>CPU:</> {{ round .PhysicalPercentUsed .Precision }}% ",
					"type": "sysinfo"
				},
				{
					"foreground": "#caffbe",
					"style": "diamond",
					"template": " <#ffffff>\ue28c</>  <#ffffff>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB ",
					"type": "sysinfo"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#cff9ff",
					"leading_diamond": "<#bfa8bf>~</>",
					"properties": {
						"folder_icon": "\u002F",
						"folder_separator_icon": " \u002F ",
						"home_icon": "home",
						"style": "agnoster_full"
					},
					"style": "diamond",
					"template": " \ue5ff  {{ .Path }}",
					"trailing_diamond": "",
					"type": "path"
				},
				{
					"foreground": "#8eff7f",
					"foreground_templates": [
						"{{ if gt .Code 0 }}#ef5350{{ end }}"
					],
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": " \u007E ",
					"type": "exit"
				}
			],
			"type": "prompt"
		}
	],
	"console_title_template": "{{ .HostName }} {{ .Folder }}",
	"osc99": true,
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#FEF5ED",
		"template": "> "
	},
	"version": 2
}